<html>
<head>
<title>iNetVOD Widget</title>
<hta:application id="inetvodwidget"
	applicationname="inetvodwidget"
	border="thin"
	caption="yes"
	showintaskbar="yes"
	singleinstance="yes"
	sysmenu="yes"
	scroll="no"
	windowstate="normal" />
<link rel="stylesheet" href="styles/style.css" type="text/css"/>
</head>
<script type="text/javascript" src="main.js"></script>
<script type="text/javascript">
function init()
{
	MainApp.getThe().init();
}
</script>
<body id="MainBody">

<!-- *********************************************************************** -->
<!-- *********************************************************************** -->

<div id="StartUp001" style="position:absolute; display:none">
<table cellpadding=0 cellspacing=0 border=0 width=390>
	<tr><td id="StartUp001_Text" class="textCtr" style="text-align:center;">
		</td></tr>
</table>
</div>

<!-- *********************************************************************** -->
<!-- *********************************************************************** -->

<div id="Show002" style="position:absolute; display:none">

<table cellpadding=0 cellspacing=0 border=0 width=390>
	<tr><td>
		<table cellpadding=0 cellspacing=3 border=0>
			<tr>
				<td id="Show002_PlayList" class="buttonCtr100_normal">Playlist</td>
				<td id="Show002_Featured" class="buttonCtr100_normal">Featured</td>
				<td width="65"><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td>
				<td id="Show002_Close" class="buttonCtr100_normal">Close</td>
			</tr>
		</table>
	</td></tr>

	<tr><td height=20><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td></tr>

	<tr><td>
		<div id="Show002_NoShowsText" style="height:166; display:none;">
			<span class="textSmallCtr" style="width:380;">Your Now Playing list is empty.</span>
		</div>

		<div id="Show002_ShowList">
		<table cellpadding=0 cellspacing=3 border=0 width=386>
			<tr><td
				><span class="listCtrHeader" style="width:380;">Playlist</span
				></td>
			</tr>
			<tr><td id="Show002_ShowList_0" class="listCtrRow380_normal"
				><span id="Show002_ShowList_0_Show" class="listCtrItem_normal"></span
				></td>
			</tr>
			<tr><td id="Show002_ShowList_1" class="listCtrRow380_normal"
				><span id="Show002_ShowList_1_Show" class="listCtrItem_normal"></span
				></td>
			</tr>
			<tr><td id="Show002_ShowList_2" class="listCtrRow380_normal"
				><span id="Show002_ShowList_2_Show" class="listCtrItem_normal"></span
				></td>
			</tr>
			<tr><td id="Show002_ShowList_3" class="listCtrRow380_normal"
				><span id="Show002_ShowList_3_Show" class="listCtrItem_normal"></span
				></td>
			</tr>
			<tr><td id="Show002_ShowList_4" class="listCtrRow380_normal"
				><span id="Show002_ShowList_4_Show" class="listCtrItem_normal"></span
				></td>
			</tr>
			<tr><td id="Show002_ShowList_5" class="listCtrRow380_normal"
				><span id="Show002_ShowList_5_Show" class="listCtrItem_normal"></span
				></td>
			</tr>
		</table>

		<table cellpadding=0 cellspacing=0 border=0 width=386><tr><td align="right" valign="top">
			<table cellpadding=0 cellspacing=6 border=0>
				<tr><td id="Show002_ShowList_Count" class="listCtrCount">0 of 0</td
					><td width=2><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td
					><td id="Show002_ShowList_Up" class="listCtrPrev_normal"></td
					><td id="Show002_ShowList_Down" class="listCtrNext_normal"></td
				></tr>
			</table>
		</td></tr></table>
		</div>
	</td></tr>

	<tr><td height=5><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td></tr>

	<tr><td>
		<table cellpadding=0 cellspacing=3 border=0>
			<tr>
				<td id="Show002_Play" class="buttonCtr100_normal">Play</td>
				<td id="Show002_Send" class="buttonCtr100_normal">Send</td>
			</tr>
		</table>
	</td></tr>

</table>
</div>

<!-- *********************************************************************** -->
<!-- *********************************************************************** -->

<div id="Recom003" style="position:absolute; display:none">
<table cellpadding=0 cellspacing=0 border=0 width=390>

	<tr><td class="textCtr" colspan="2">Recommend</td></tr>
	<tr><td height="5"><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td></tr>
	<tr>
		<td width="20"><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td>
		<td><span id="Recom003_ShowName" class="textDataCtr" style="width:330;"></span></td>
	</tr>

	<tr><td height="20"><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td></tr>

	<tr><td class="textCtr" colspan="2">Enter email address:</td></tr>
	<tr><td height="5"><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td></tr>
	<tr>
		<td width="20"><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td>
		<td
		><table cellpadding=0 cellspacing=6 border=0
			><tr><td><span id="Recom003_Email" class="editCtr306_normal"
					><span id="Recom003_Email_0" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_1" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_2" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_3" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_4" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_5" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_6" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_7" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_8" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_9" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_10" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_11" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_12" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_13" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_14" class="editCtrCharAlpha_normal"></span
					><span id="Recom003_Email_15" class="editCtrCharAlpha_normal"></span
				></span></td
			></tr
		></table>
		<br>
		<br>
	</td></tr>
	<tr><td align="center" colspan="2"
		><table cellpadding=0 cellspacing=0 border=0
			><tr><td id="Recom003_Cancel" class="buttonCtr100_normal">Cancel</td>
				<td width="5"><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td>
				<td id="Recom003_Send" class="buttonCtr100_normal">Send</td></tr
		></table
	></td></tr>
</table>
</div>

<!-- *********************************************************************** -->
<!-- *********************************************************************** -->

<div id="Wait001" style="position:absolute; left:0; top:0; display:none;">

<div id="Wait001_Backround" style="position:absolute; left:0; top:0;">
<table width=400 height=400 cellpadding=0 cellspacing=0 border=0>
	<tr><td style="background-color:#222222; filter:alpha(opacity=70)">&nbsp;</td></tr>
</table>
</div>

<div id="Wait001_Image" style="position:absolute; left:0; top:0;">
<table width="100%" cellpadding=0 cellspacing=0 border=0>
	<tr><td height=190><img src="images/spacer.gif" border=0 width=1 height=1 alt=""></td></tr>
	<tr><td align=center valign=middle>
		<img src="images/wait.gif" alt="Please wait...">
	</td></tr>
</table>
</div>

</div>

<!-- *********************************************************************** -->
<!-- *********************************************************************** -->

<table width="100%" height="100%" cellpadding=0 cellspacing=0 border=0>
	<tr><td align=left valign=top>
<table id="MainTable" width=410 height=400 class="tableMain" cellpadding=0 cellspacing=0 border=0>
	<tr>
		<td></td>
	</tr>
</table>
	</td></tr>
</table>

<!-- *********************************************************************** -->
<!-- *********************************************************************** -->

<div id="ScaleDiv" style="position:absolute; left:10; top:10; display:none; font-size:8pt; background-color:white;">
<a id="ScaleLink" href="javascript:tryit('DoScale();');">Scale</a><br>
<span id="ScaleText"></span>
</div>

<!-- *********************************************************************** -->
<!-- *********************************************************************** -->

<div id="Debugger" style="position:absolute; left:0; top:0; top:50; display:none; font-size:8pt; background-color:white;">
<a href="javascript:tryit('DebugShow();');">Debug</a>&nbsp;
<a href="javascript:tryit('DebugClear();');">Clear</a><br>
<span id="Debugger_Out"></span>
</div>

<!-- *********************************************************************** -->
<!-- *********************************************************************** -->

</body>
</html>
<script for=document event=onkeyup type="text/javascript">
	return !MainAppOnKeyUp();
</script>
<script for=document event=onkeydown type="text/javascript">
	return !MainAppOnKeyDown();
</script>
<script for=document event=onkeypress type="text/javascript">
	return !MainAppOnKeyPress();
</script>
<script for=window event=onresize type="text/javascript">
	MainAppOnResize();
</script>
<script for=window event=onload type="text/javascript">
	tryit('init();');
</script>
